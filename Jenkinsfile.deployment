@Library(value="COPS-4904") _

pipeline {
	agent none
   	
    options {
        disableConcurrentBuilds()
    }
   	
   	environment {
		// GLobal Vars
		IMAGE_NAME = "climate-summary"
    }

    stages {
    	stage('deploy') {
			steps {
				script {
    				deploy("${IMAGE_NAME}", true)
    			}
    		}
    	}
    }
}