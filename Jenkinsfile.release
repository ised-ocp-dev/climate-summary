@Library('ised-cicd-lib','COPS-4904') _

pipeline {
	agent {
		label 'maven'
   	}
   	
    options {
        disableConcurrentBuilds()
    }
    
   	environment {
		// GLobal Vars
		IMAGE_NAME = "climate-summary"
    }
    
    stages {
    	stage('release') {
			steps {
				script{
					release.releaseMaven("${IMAGE_NAME}")
		    	}
			}
    	}
    }
}